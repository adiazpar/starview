#: starview_app/templates/account/email/email_confirmation_signup_message.html:8
#: starview_app/templates/account/email/email_confirmation_signup_subject.txt:3
msgid "Verify Your Starview Account"
msgstr "Vérifiez Votre Compte Starview"

#: starview_app/templates/account/email/email_confirmation_signup_message.html:185
